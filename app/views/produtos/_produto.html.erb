<div id="<%= dom_id produto %>">
  <p>
    <strong>Nome:</strong>
    <%= produto.nome %>
  </p>

  <p>
    <strong>Descricao:</strong>
    <%= produto.descricao %>
  </p>

  <p>
    <strong>Preco unitario:</strong>
    <%= produto.preco_unitario %>
  </p>

  <p>
    <strong>Preco custo:</strong>
    <%= produto.preco_custo %>
  </p>

  <p>
    <strong>Categoria:</strong>
    <%= produto.categoria_id %>
  </p>

</div>
